

public class InsertionSort {
    public static void main(String[] args) {
        int[] arr= {6,4,8,1};

        for(int i=1;i<5;i++){
            int m=arr[i];
            int j=i-1;
            if(j>=0 && m<arr[j]){
            arr[j+1]=arr[j];
             j--;
        
            }
            arr[j]=m;
        }
            for(int i=1;i<5;i++){

            
            System.out.println(arr[i]);
            }
        
    }
}
