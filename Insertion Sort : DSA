 public class InsertionSort{
    public static void main(String[] args) {
        int[] arr={5,1,9,0};

        for(int i=1;i<=3;i++){
            int b=arr[i];
            int j=i-1;

            while(j>=0 && arr[j]>b){
                arr[j+1]=arr[j];
                j--;
            }
             
             arr[j+1]=b;

        }
        for(int i=0;i<4;i++){
          System.out.println(arr[i]);
        }
    }
}
